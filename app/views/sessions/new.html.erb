<%= form_for :session, :url => login_path, :method => :post do |f| %> 	


  <%= f.label "Email address" %>
  <%= f.text_field :email %><br/><br/>
   <%= f.label "Password" %>
  <%= f.password_field :password %><br/>


  <div class="actions">
    <br/>
    <%= f.submit "Log in" %>
  </div>

<% end %>